{
  "name": "malariaelectronui",
  "version": "1.0.0",
  "description": "connects malaria cell detection model with electron ui",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
   "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
"package-win": "electron-packager . Malaria_Cell_App --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Malaria_Cell_App\"",    
"package-linux": "electron-packager . Malaria_Cell_App --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
},
  "author": "Mario Weidner",
  "license": "MIT",
  "dependencies": {
    "python-shell": "^1.0.7"
  },
  "devDependencies": {
    "electron-packager": "^13.1.1",
    "electron": "^13.6.6"
  }
}
